{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/CoreKitRenderers",
    "definitions": {
        "CoreKitRenderers": {
            "anyOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "renderer"
                        },
                        "subtype": {
                            "type": "string",
                            "const": "group"
                        },
                        "children": {
                            "$ref": "#/definitions/ChildType"
                        }
                    },
                    "required": [
                        "children",
                        "subtype",
                        "type"
                    ]
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "renderer"
                        },
                        "subtype": {
                            "type": "string",
                            "const": "rawText"
                        },
                        "content": {
                            "$ref": "#/definitions/ValueRoot"
                        }
                    },
                    "required": [
                        "content",
                        "subtype",
                        "type"
                    ]
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "renderer"
                        },
                        "subtype": {
                            "type": "string",
                            "const": "rawInput"
                        },
                        "field": {
                            "type": "boolean"
                        },
                        "fieldSet": {
                            "$ref": "#/definitions/ValueRoot"
                        },
                        "placeholder": {
                            "$ref": "#/definitions/ValueRoot"
                        }
                    },
                    "required": [
                        "field",
                        "fieldSet",
                        "subtype",
                        "type"
                    ]
                }
            ]
        },
        "ChildType": {
            "anyOf": [
                {
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "$ref": "#/definitions/RendererKitItem"
                            },
                            {
                                "$ref": "#/definitions/GeneratorItems"
                            }
                        ]
                    }
                },
                {
                    "$ref": "#/definitions/RendererKitItem"
                },
                {
                    "$ref": "#/definitions/GeneratorItems"
                }
            ]
        },
        "RendererKitItem": {
            "anyOf": [
                {
                    "$ref": "#/definitions/RendererKitItemNoForm"
                },
                {
                    "$ref": "#/definitions/RendererKitItemForm"
                }
            ]
        },
        "RendererKitItemNoForm": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "renderer"
                },
                "subtype": {
                    "type": "string"
                }
            },
            "required": [
                "subtype",
                "type"
            ]
        },
        "RendererKitItemForm": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "renderer"
                },
                "subtype": {
                    "type": "string"
                },
                "field": {
                    "type": "boolean"
                },
                "fieldSet": {
                    "$ref": "#/definitions/ValueRoot"
                }
            },
            "required": [
                "field",
                "fieldSet",
                "subtype",
                "type"
            ]
        },
        "ValueRoot": {
            "anyOf": [
                {
                    "$ref": "#/definitions/Literal"
                },
                {
                    "$ref": "#/definitions/ValueKitItem"
                }
            ]
        },
        "Literal": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "number"
                },
                {
                    "type": "boolean"
                },
                {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Literal"
                    }
                },
                {
                    "type": "null"
                }
            ]
        },
        "ValueKitItem": {
            "anyOf": [
                {
                    "$ref": "#/definitions/DataValueItem"
                },
                {
                    "$ref": "#/definitions/TextValueItem"
                },
                {
                    "$ref": "#/definitions/FunctionValueItem"
                },
                {
                    "type": "string"
                },
                {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ValueRoot"
                    }
                }
            ]
        },
        "DataValueItem": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "value"
                },
                "subtype": {
                    "type": "string",
                    "const": "data"
                },
                "path": {
                    "$ref": "#/definitions/ValueKitItem"
                },
                "default": {
                    "$ref": "#/definitions/ValueRoot"
                }
            },
            "required": [
                "type",
                "subtype",
                "path"
            ],
            "additionalProperties": false
        },
        "TextValueItem": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "value"
                },
                "subtype": {
                    "type": "string",
                    "const": "text"
                },
                "text": {
                    "$ref": "#/definitions/ValueRoot"
                },
                "substitutions": {
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/ValueRoot"
                    }
                }
            },
            "required": [
                "type",
                "subtype",
                "text",
                "substitutions"
            ],
            "additionalProperties": false
        },
        "FunctionValueItem": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "value"
                },
                "subtype": {
                    "type": "string",
                    "const": "function"
                },
                "function": {
                    "$ref": "#/definitions/FunctionType"
                }
            },
            "required": [
                "type",
                "subtype",
                "function"
            ],
            "additionalProperties": false
        },
        "FunctionType": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "opts": {
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/ValueRoot"
                    }
                }
            },
            "required": [
                "code",
                "opts"
            ],
            "additionalProperties": false
        },
        "GeneratorItems": {
            "$ref": "#/definitions/IteratorGenerator"
        },
        "IteratorGenerator": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "generator"
                },
                "subtype": {
                    "type": "string",
                    "const": "iterator"
                },
                "iterator": {
                    "$ref": "#/definitions/ValueRoot"
                },
                "renderer": {
                    "$ref": "#/definitions/ChildType"
                }
            },
            "required": [
                "iterator",
                "renderer",
                "subtype",
                "type"
            ],
            "additionalProperties": false
        }
    }
}